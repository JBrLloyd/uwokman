{
  "name": "public-spotify-playing-api",
  "version": "0.1.0",
  "description": "A starting point for Node.js express apps with TypeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/jbrlloyd/uwokman"
  },
  "author": "Jackson Lloyd",
  "license": "MIT",
  "scripts": {
    "build:webpack": "webpack",
    "build": "npm run lint && npm run build:webpack",
    "dev": "npm run build && npm run watch-debug",
    "lint": "npx tsc --noEmit && eslint \"src/**/*.{js,ts}\" --quiet --fix",
    "serve-debug": "nodemon --inspect dist/server.js",
    "start": "node dist/server.js",
    "start:dev": "webpack-dev-server",
    "test": "jest --forceExit --coverage --verbose",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Webpack,Node\" -c \"cyan.bold,green.bold\" \"npm run start:dev\" \"npm run serve-debug\"",
    "watch-debug-old": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "nodemon dist/src/server.js",
    "watch-webpack": "webpack --watch",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "npx tsc -w"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "async": "^3.1.0",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-handlebars": "^5.1.0",
    "express-rate-limit": "^5.1.3",
    "express-validator": "^6.2.0",
    "helmet": "^4.1.1",
    "lodash": "^4.17.19",
    "lusca": "^1.6.1",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.1",
    "query-string": "^6.13.1",
    "retry": "^0.12.0",
    "swagger-ui-express": "^4.1.4",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/async": "^3.0.2",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/bluebird": "^3.5.27",
    "@types/body-parser": "^1.17.1",
    "@types/chai": "^4.2.3",
    "@types/compression": "^1.0.1",
    "@types/concurrently": "^4.1.0",
    "@types/cors": "^2.8.7",
    "@types/errorhandler": "^0.0.32",
    "@types/eslint": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/express-flash": "0.0.1",
    "@types/express-handlebars": "^3.1.0",
    "@types/express-rate-limit": "^5.1.0",
    "@types/express-session": "^1.15.14",
    "@types/jest": "^24.0.23",
    "@types/jquery": "^3.3.31",
    "@types/lodash": "^4.14.141",
    "@types/lusca": "^1.6.1",
    "@types/node": "^12.7.8",
    "@types/node-fetch": "^2.5.7",
    "@types/node-sass": "^4.11.0",
    "@types/nodemailer": "^6.2.1",
    "@types/opn": "^5.5.0",
    "@types/passport": "^1.0.1",
